{
    "name": "distilleries/mailersaver",
    "description": "Mailer saver extend the laravel 5 mailer. Add the possibility to override the to,cc,bcc of your mail without modify your implementation. Add the possibility to get your template mail, subject, cc, bcc and type from a model.",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxime Francois",
            "email": "kezho@me.com"
        }
    ],

    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.*",
        "illuminate/database": "5.0.*",
        "illuminate/view": "5.0.*",
        "illuminate/mail": "5.0.*",
        "illuminate/filesystem": "5.0.*",
        "wpb/string-blade-compiler": "2.0.0"
    },

    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "Distilleries\\MailerSaver\\": "src/Distilleries/MailerSaver"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "orchestra/testbench": "3.0.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.5",
        "orchestra/database": "~3.0"
    }
}
