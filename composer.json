{
    "name": "distilleries/mailersaver",
    "description": "Override helper Mailer from laravel to hook the sender",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxime Francois",
            "email": "kezho@me.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mikerogne/StringBladeCompiler",
            "no-api": true
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.7.*",
        "illuminate/database": "5.7.*",
        "illuminate/view": "5.7.*",
        "illuminate/mail": "5.7.*",
        "illuminate/filesystem": "5.7.*",
        "wpb/string-blade-compiler": "dev-laravel-5.7#d9cc177760727fa7ac4df0a322cc3e28c0c074c7"
    },

    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "Distilleries\\MailerSaver\\": "src/Distilleries/MailerSaver"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "3.7.*",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "github-protocols": ["https"]
    }
}
